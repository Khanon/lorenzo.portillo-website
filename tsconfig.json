{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "esNext",
        "target": "esNext",
        "lib": [
            "dom",
            "es6",
        ],
        "importHelpers": true,
        "declaration": true,
        "noResolve": false,
        "noImplicitThis": true,
        "noImplicitAny": false,
        "noImplicitReturns": false,
        "strictFunctionTypes": true,
        "skipLibCheck": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "sourceMap": true,
        "preserveConstEnums":true
    }
}
